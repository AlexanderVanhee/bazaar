<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="Adw" version="1.0"/>
  <template class="BzSearchWidget" parent="AdwBin">
    <child>
      <object class='GtkShortcutController'>
        <child>
          <object class='GtkShortcut'>
            <property name='trigger'>&lt;primary&gt;p</property>
            <property name='action'>action(move)</property>
            <property name='arguments'>-1</property>
          </object>
        </child>
        <child>
          <object class='GtkShortcut'>
            <property name='trigger'>&lt;primary&gt;n</property>
            <property name='action'>action(move)</property>
            <property name='arguments'>1</property>
          </object>
        </child>
        <child>
          <object class='GtkShortcut'>
            <property name='trigger'>&lt;primary&gt;k</property>
            <property name='action'>action(move)</property>
            <property name='arguments'>-1</property>
          </object>
        </child>
        <child>
          <object class='GtkShortcut'>
            <property name='trigger'>&lt;primary&gt;j</property>
            <property name='action'>action(move)</property>
            <property name='arguments'>1</property>
          </object>
        </child>
        <child>
          <object class='GtkShortcut'>
            <property name='trigger'>&lt;primary&gt;&lt;alt&gt;p</property>
            <property name='action'>action(move)</property>
            <property name='arguments'>-10</property>
          </object>
        </child>
        <child>
          <object class='GtkShortcut'>
            <property name='trigger'>&lt;primary&gt;&lt;alt&gt;n</property>
            <property name='action'>action(move)</property>
            <property name='arguments'>10</property>
          </object>
        </child>
        <child>
          <object class='GtkShortcut'>
            <property name='trigger'>&lt;primary&gt;&lt;alt&gt;k</property>
            <property name='action'>action(move)</property>
            <property name='arguments'>-10</property>
          </object>
        </child>
        <child>
          <object class='GtkShortcut'>
            <property name='trigger'>&lt;primary&gt;&lt;alt&gt;j</property>
            <property name='action'>action(move)</property>
            <property name='arguments'>10</property>
          </object>
        </child>
      </object>
    </child>
    <property name="child">
      <object class="GtkBox">
        <style>
          <class name="view"/>
        </style>
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkBox">
            <property name="css-name">search</property>
            <property name="orientation">horizontal</property>
            <child>
              <object class="GtkImage">
                <property name="icon-name">edit-find-symbolic</property>
                <property name="pixel-size">24</property>
                <property name="margin-start">6</property>
                <property name="margin-end">6</property>
              </object>
            </child>
            <child>
              <object class="GtkText" id="search_bar">
                <property name="hexpand">TRUE</property>
                <property name="placeholder-text" translatable="yes">Type to filter</property>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="search_text"/>
            </child>
            <child>
              <object class="GtkLabel" id="regex_error">
                <style>
                  <class name="error"/>
                  <class name="body"/>
                  <class name="monospace"/>
                </style>
                <property name="has-tooltip">TRUE</property>
                <property name="ellipsize">start</property>
              </object>
            </child>
            <child>
              <object class="AdwSpinner" id="search_spinner">
                <property name="visible">FALSE</property>
              </object>
            </child>
            <child>
              <object class="GtkToggleButton" id="regex_toggle">
                <property name="margin-start">6</property>
                <property name="margin-end">6</property>
                <property name="has-tooltip">TRUE</property>
                <property name="tooltip-text">Whether to match using regular expressions</property>
                <property name="icon-name">regex-symbolic</property>
              </object>
            </child>
            <child>
              <object class="GtkButton">
                <property name="icon-name">window-close-symbolic</property>
                <property name="tooltip-text">Close Miniwindow</property>
                <property name="valign">center</property>
                <property name="action-name">sheet.close</property>
                <style>
                  <class name="close"/>
                  <class name="circular"/>
                </style>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="AdwBreakpointBin">
            <property name="width-request">300</property>
            <property name="height-request">300</property>
            <property name="child">
              <object class="GtkBox" id="content_box">
                <property name="orientation">horizontal</property>
                <binding name="visible">
                  <closure function="invert_boolean" type="gboolean">
                    <closure function="is_null" type="gboolean">
                      <lookup name="selected-item" type="GtkSingleSelection">
                        <lookup name="model">list_view</lookup>
                      </lookup>
                    </closure>
                  </closure>
                </binding>
                <child>
                  <object class="GtkBox" id="entry_view">
                    <property name="vexpand">TRUE</property>
                    <property name="hexpand">FALSE</property>
                    <property name="margin-start">10</property>
                    <property name="margin-end">10</property>
                    <property name="margin-top">10</property>
                    <property name="margin-bottom">10</property>
                    <property name="orientation">vertical</property>
                    <property name="spacing">10</property>
                    <child>
                      <object class="GtkBox">
                        <property name="vexpand">FALSE</property>
                        <property name="orientation">horizontal</property>
                        <property name="spacing">10</property>
                        <child>
                          <object class="GtkBox">
                            <property name="orientation">vertical</property>
                            <property name="spacing">25</property>
                            <child>
                              <object class="GtkPicture">
                                <property name="width-request">130</property>
                                <property name="content-fit">scale-down</property>
                                <property name="can-shrink">FALSE</property>
                                <binding name="paintable">
                                  <lookup name="icon-paintable" type="BzEntry">
                                    <lookup name="selected-item" type="GtkSingleSelection">
                                      <lookup name="model">list_view</lookup>
                                    </lookup>
                                  </lookup>
                                </binding>
                              </object>
                            </child>
                            <child>
                              <object class="GtkBox">
                                <property name="orientation">vertical</property>
                                <property name="spacing">5</property>
                                <child>
                                  <object class="GtkBox">
                                    <property name="orientation">horizontal</property>
                                    <property name="spacing">10</property>
                                    <child>
                                      <object class="GtkImage">
                                        <style>
                                          <class name="good-icon"/>
                                        </style>
                                        <property name="has-tooltip">TRUE</property>
                                        <property name="tooltip-text">This application has a FLOSS license, meaning the source code can be audited for safety.</property>
                                        <property name="icon-name">license-symbolic</property>
                                        <binding name="visible">
                                          <lookup name="is-floss" type="BzEntry">
                                            <lookup name="selected-item" type="GtkSingleSelection">
                                              <lookup name="model">list_view</lookup>
                                            </lookup>
                                          </lookup>
                                        </binding>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="GtkImage">
                                        <style>
                                          <class name="bad-icon"/>
                                        </style>
                                        <property name="has-tooltip">TRUE</property>
                                        <property name="tooltip-text">This application has a proprietary license, meaning the source code cannot be audited and your privacy is at risk.</property>
                                        <property name="icon-name">dialog-warning-symbolic</property>
                                        <binding name="visible">
                                          <closure function="invert_boolean" type="gboolean">
                                            <lookup name="is-floss" type="BzEntry">
                                              <lookup name="selected-item" type="GtkSingleSelection">
                                                <lookup name="model">list_view</lookup>
                                              </lookup>
                                            </lookup>
                                          </closure>
                                        </binding>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="GtkLabel">
                                        <property name="has-tooltip">TRUE</property>
                                        <property name="xalign">0</property>
                                        <property name="selectable">TRUE</property>
                                        <property name="ellipsize">end</property>
                                        <property name="width-chars">17</property>
                                        <property name="max-width-chars">17</property>
                                        <binding name="tooltip-text">
                                          <lookup name="project-license" type="BzEntry">
                                            <lookup name="selected-item" type="GtkSingleSelection">
                                              <lookup name="model">list_view</lookup>
                                            </lookup>
                                          </lookup>
                                        </binding>
                                        <binding name="label">
                                          <lookup name="project-license" type="BzEntry">
                                            <lookup name="selected-item" type="GtkSingleSelection">
                                              <lookup name="model">list_view</lookup>
                                            </lookup>
                                          </lookup>
                                        </binding>
                                      </object>
                                    </child>
                                  </object>
                                </child>
                                <child>
                                  <object class="GtkBox">
                                    <property name="orientation">horizontal</property>
                                    <property name="spacing">10</property>
                                    <child>
                                      <object class="GtkImage">
                                        <style>
                                          <class name="neutral-icon"/>
                                        </style>
                                        <property name="has-tooltip">TRUE</property>
                                        <property name="tooltip-text">Remote repo name</property>
                                        <property name="icon-name">globe-symbolic</property>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="GtkLabel">
                                        <property name="has-tooltip">TRUE</property>
                                        <property name="xalign">0</property>
                                        <property name="selectable">TRUE</property>
                                        <property name="ellipsize">end</property>
                                        <property name="width-chars">17</property>
                                        <property name="max-width-chars">17</property>
                                        <binding name="tooltip-text">
                                          <lookup name="remote-repo-name" type="BzEntry">
                                            <lookup name="selected-item" type="GtkSingleSelection">
                                              <lookup name="model">list_view</lookup>
                                            </lookup>
                                          </lookup>
                                        </binding>
                                        <binding name="label">
                                          <lookup name="remote-repo-name" type="BzEntry">
                                            <lookup name="selected-item" type="GtkSingleSelection">
                                              <lookup name="model">list_view</lookup>
                                            </lookup>
                                          </lookup>
                                        </binding>
                                      </object>
                                    </child>
                                  </object>
                                </child>
                                <child>
                                  <object class="GtkBox">
                                    <property name="orientation">horizontal</property>
                                    <property name="spacing">10</property>
                                    <child>
                                      <object class="GtkImage">
                                        <style>
                                          <class name="neutral-icon"/>
                                        </style>
                                        <property name="has-tooltip">TRUE</property>
                                        <property name="tooltip-text">Project URL</property>
                                        <property name="icon-name">external-link-symbolic</property>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="GtkLabel">
                                        <property name="has-tooltip">TRUE</property>
                                        <property name="xalign">0</property>
                                        <property name="use-markup">TRUE</property>
                                        <property name="ellipsize">start</property>
                                        <property name="width-chars">17</property>
                                        <property name="max-width-chars">17</property>
                                        <binding name="label">
                                          <closure function="format_as_link" type="gchararray">
                                            <lookup name="url" type="BzEntry">
                                              <lookup name="selected-item" type="GtkSingleSelection">
                                                <lookup name="model">list_view</lookup>
                                              </lookup>
                                            </lookup>
                                          </closure>
                                        </binding>
                                      </object>
                                    </child>
                                  </object>
                                </child>
                                <child>
                                  <object class="GtkBox">
                                    <property name="orientation">horizontal</property>
                                    <property name="spacing">10</property>
                                    <child>
                                      <object class="GtkImage">
                                        <style>
                                          <class name="neutral-icon"/>
                                        </style>
                                        <property name="has-tooltip">TRUE</property>
                                        <property name="tooltip-text">Download size</property>
                                        <property name="icon-name">drive-harddisk-symbolic</property>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="GtkLabel">
                                        <property name="has-tooltip">TRUE</property>
                                        <property name="xalign">0</property>
                                        <property name="selectable">TRUE</property>
                                        <property name="ellipsize">end</property>
                                        <property name="width-chars">17</property>
                                        <property name="max-width-chars">17</property>
                                        <binding name="tooltip-text">
                                          <closure function="format_size" type="gchararray">
                                            <lookup name="size" type="BzEntry">
                                              <lookup name="selected-item" type="GtkSingleSelection">
                                                <lookup name="model">list_view</lookup>
                                              </lookup>
                                            </lookup>
                                          </closure>
                                        </binding>
                                        <binding name="label">
                                          <closure function="format_size" type="gchararray">
                                            <lookup name="size" type="BzEntry">
                                              <lookup name="selected-item" type="GtkSingleSelection">
                                                <lookup name="model">list_view</lookup>
                                              </lookup>
                                            </lookup>
                                          </closure>
                                        </binding>
                                      </object>
                                    </child>
                                  </object>
                                </child>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkBox" id="right_box">
                            <property name="orientation">vertical</property>
                            <property name="spacing">5</property>
                            <child>
                              <object class="GtkLabel" id="title_label">
                                <style>
                                  <class name="title-1"/>
                                </style>
                                <property name="width-chars">20</property>
                                <property name="max-width-chars">20</property>
                                <property name="wrap">TRUE</property>
                                <property name="wrap-mode">word-char</property>
                                <property name="xalign">0</property>
                                <property name="selectable">TRUE</property>
                                <binding name="label">
                                  <lookup name="title" type="BzEntry">
                                    <lookup name="selected-item" type="GtkSingleSelection">
                                      <lookup name="model">list_view</lookup>
                                    </lookup>
                                  </lookup>
                                </binding>
                              </object>
                            </child>
                            <child>
                              <object class="GtkLabel" id="developer_label">
                                <style>
                                  <class name="caption-heading"/>
                                  <class name="accent"/>
                                </style>
                                <property name="width-chars">30</property>
                                <property name="max-width-chars">30</property>
                                <property name="wrap">TRUE</property>
                                <property name="wrap-mode">word-char</property>
                                <property name="xalign">0</property>
                                <property name="selectable">TRUE</property>
                                <binding name="label">
                                  <lookup name="developer" type="BzEntry">
                                    <lookup name="selected-item" type="GtkSingleSelection">
                                      <lookup name="model">list_view</lookup>
                                    </lookup>
                                  </lookup>
                                </binding>
                              </object>
                            </child>
                            <child>
                              <object class="GtkLabel" id="description_label">
                                <style>
                                  <class name="title-3"/>
                                </style>
                                <property name="width-chars">30</property>
                                <property name="max-width-chars">30</property>
                                <property name="wrap">TRUE</property>
                                <property name="wrap-mode">word-char</property>
                                <property name="xalign">0</property>
                                <property name="selectable">TRUE</property>
                                <binding name="label">
                                  <lookup name="description" type="BzEntry">
                                    <lookup name="selected-item" type="GtkSingleSelection">
                                      <lookup name="model">list_view</lookup>
                                    </lookup>
                                  </lookup>
                                </binding>
                              </object>
                            </child>
                            <child>
                              <object class="GtkScrolledWindow" id="long_desc_scroll">
                                <style>
                                  <class name="long-description"/>
                                </style>
                                <property name="vexpand">TRUE</property>
                                <property name="hscrollbar-policy">never</property>
                                <property name="child">
                                  <object class="GtkLabel" id="long_description">
                                    <property name="margin-start">10</property>
                                    <property name="margin-end">10</property>
                                    <property name="margin-top">10</property>
                                    <property name="margin-bottom">10</property>
                                    <property name="valign">start</property>
                                    <property name="xalign">0</property>
                                    <property name="selectable">TRUE</property>
                                    <property name="wrap">TRUE</property>
                                    <property name="wrap-mode">word-char</property>
                                    <property name="use-markup">TRUE</property>
                                    <binding name="label">
                                      <lookup name="long-description" type="BzEntry">
                                        <lookup name="selected-item" type="GtkSingleSelection">
                                          <lookup name="model">list_view</lookup>
                                        </lookup>
                                      </lookup>
                                    </binding>
                                  </object>
                                </property>
                              </object>
                            </child>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="GtkScrolledWindow" id="screenshot_scroll">
                        <style>
                          <class name="screenshot-scroll"/>
                        </style>
                        <property name="width-request">140</property>
                        <property name="vexpand">TRUE</property>
                        <property name="propagate-natural-height">TRUE</property>
                        <property name="hscrollbar-policy">never</property>
                        <property name="child">
                          <object class="GtkBox">
                            <property name="orientation">vertical</property>
                            <child>
                              <object class="AdwSpinner">
                                <property name="height-request">150</property>
                                <binding name="visible">
                                  <closure function="is_null" type="gboolean">
                                    <lookup name="previewing">BzSearchWidget</lookup>
                                  </closure>
                                </binding>
                              </object>
                            </child>
                            <child>
                              <object class="GtkBox">
                                <property name="orientation">horizontal</property>
                                <property name="spacing">5</property>
                                <property name="hexpand">TRUE</property>
                                <property name="halign">center</property>
                                <child>
                                  <object class="GtkLabel">
                                    <style>
                                      <class name="dimmed"/>
                                    </style>
                                    <property name="margin-top">5</property>
                                    <property name="label">Click on an image to open the screenshots externally.</property>
                                    <binding name="visible">
                                      <closure function="invert_boolean" type="gboolean">
                                        <closure function="is_null" type="gboolean">
                                          <lookup name="previewing">BzSearchWidget</lookup>
                                        </closure>
                                      </closure>
                                    </binding>
                                  </object>
                                </child>
                                <child>
                                  <object class="AdwSpinner" id="loading_screenshots_external">
                                    <property name="visible">FALSE</property>
                                  </object>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkLabel" id="open_screenshot_error">
                                <style>
                                  <class name="error"/>
                                </style>
                                <property name="margin-top">5</property>
                                <property name="visible">FALSE</property>
                                <property name="selectable">TRUE</property>
                              </object>
                            </child>
                            <child>
                              <object class="GtkListView" id="screenshots">
                                <style>
                                  <class name="navigation-sidebar"/>
                                </style>
                                <property name="margin-start">5</property>
                                <property name="margin-end">5</property>
                                <property name="single-click-activate">TRUE</property>
                                <property name="model">
                                  <object class="GtkNoSelection">
                                    <binding name="model">
                                      <lookup name="screenshot-paintables" type="BzEntry">
                                        <lookup name="previewing">BzSearchWidget</lookup>
                                      </lookup>
                                    </binding>
                                  </object>
                                </property>
