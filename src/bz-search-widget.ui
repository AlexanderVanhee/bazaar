                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkSeparator" id="main_separator">
                    <property name="visible">FALSE</property>
                  </object>
                </child>
                <child>
                  <object class="GtkScrolledWindow" id="entry_list_scroll">
